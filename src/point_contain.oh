#ifndef POINT_CONTAIN_OH
#define POINT_CONTAIN_OH

#include <obliv.oh>

typedef struct OblivPoint {
    obliv float x;
    obliv float y;
}OPoint;

typedef struct Point {
    float x;
    float y;
}Point;

typedef struct OblivVector {
    OPoint begin;
    OPoint end;
}OVector;

void point_contain(obliv int*, OPoint *p, OVector *v, int size);
void point_contain_opt(obliv int*, OPoint *p, OVector *v, int size);

#endif 